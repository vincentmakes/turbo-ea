      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="262" y="142" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="244" y="185" width="72" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Post Position -->
      <bpmndi:BPMNShape id="Task_PostPosition_di" bpmnElement="Task_PostPosition">
        <dc:Bounds x="370" y="120" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Screen Candidates -->
      <bpmndi:BPMNShape id="Task_ScreenCandidates_di" bpmnElement="Task_ScreenCandidates">
        <dc:Bounds x="370" y="305" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Interview -->
      <bpmndi:BPMNShape id="Task_Interview_di" bpmnElement="Task_Interview">
        <dc:Bounds x="550" y="120" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Make Offer -->
      <bpmndi:BPMNShape id="Task_MakeOffer_di" bpmnElement="Task_MakeOffer">
        <dc:Bounds x="730" y="120" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Onboard -->
      <bpmndi:BPMNShape id="Task_Onboard_di" bpmnElement="Task_Onboard">
        <dc:Bounds x="730" y="305" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Provision Access -->
      <bpmndi:BPMNShape id="Task_ProvisionAccess_di" bpmnElement="Task_ProvisionAccess">
        <dc:Bounds x="730" y="475" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- End Event -->
      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <dc:Bounds x="912" y="497" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="889" y="540" width="82" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Edges -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="298" y="160" />
        <di:waypoint x="370" y="160" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="420" y="200" />
        <di:waypoint x="420" y="305" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="470" y="345" />
        <di:waypoint x="510" y="345" />
        <di:waypoint x="510" y="160" />
        <di:waypoint x="550" y="160" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="650" y="160" />
