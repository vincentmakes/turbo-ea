      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_IT_di" bpmnElement="Lane_IT" isHorizontal="true">
        <dc:Bounds x="190" y="430" width="1020" height="170" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

