<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_OrderToCash"
                  targetNamespace="http://turbo-ea.io/bpmn"
                  exporter="Turbo EA"
                  exporterVersion="1.0">

  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_1" name="Order to Cash" processRef="Process_1" />
  </bpmn:collaboration>

  <bpmn:process id="Process_1" isExecutable="false">
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_Sales" name="Sales">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReceiveOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CheckCredit</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Credit</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RejectOrder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Rejected</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Warehouse" name="Warehouse">
        <bpmn:flowNodeRef>Task_PickPack</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Ship</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Finance" name="Finance">
        <bpmn:flowNodeRef>Task_Invoice</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReceivePayment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Complete</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <bpmn:startEvent id="StartEvent_1" name="Order Received">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:userTask id="Task_ReceiveOrder" name="Receive Order">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_CheckCredit" name="Check Credit">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_Credit" name="Credit OK?">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_CreditOK</bpmn:outgoing>
      <bpmn:outgoing>Flow_CreditFailed</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:userTask id="Task_RejectOrder" name="Reject Order">
      <bpmn:incoming>Flow_CreditFailed</bpmn:incoming>
      <bpmn:outgoing>Flow_ToRejectedEnd</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="EndEvent_Rejected" name="Order Rejected">
      <bpmn:incoming>Flow_ToRejectedEnd</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:userTask id="Task_PickPack" name="Pick &amp; Pack">
      <bpmn:incoming>Flow_CreditOK</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_Ship" name="Ship">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_Invoice" name="Invoice">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:userTask id="Task_ReceivePayment" name="Receive Payment">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="EndEvent_Complete" name="Order Complete">
      <bpmn:incoming>Flow_7</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_ReceiveOrder" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_ReceiveOrder" targetRef="Task_CheckCredit" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_CheckCredit" targetRef="Gateway_Credit" />
    <bpmn:sequenceFlow id="Flow_CreditOK" name="OK" sourceRef="Gateway_Credit" targetRef="Task_PickPack" />
    <bpmn:sequenceFlow id="Flow_CreditFailed" name="Failed" sourceRef="Gateway_Credit" targetRef="Task_RejectOrder" />
    <bpmn:sequenceFlow id="Flow_ToRejectedEnd" sourceRef="Task_RejectOrder" targetRef="EndEvent_Rejected" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_PickPack" targetRef="Task_Ship" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_Ship" targetRef="Task_Invoice" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_Invoice" targetRef="Task_ReceivePayment" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_ReceivePayment" targetRef="EndEvent_Complete" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_1">

      <bpmndi:BPMNShape id="Participant_1_di" bpmnElement="Participant_1" isHorizontal="true">
        <dc:Bounds x="160" y="60" width="1200" height="540" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Sales_di" bpmnElement="Lane_Sales" isHorizontal="true">
        <dc:Bounds x="190" y="60" width="1170" height="200" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Warehouse_di" bpmnElement="Lane_Warehouse" isHorizontal="true">
        <dc:Bounds x="190" y="260" width="1170" height="160" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Finance_di" bpmnElement="Lane_Finance" isHorizontal="true">
        <dc:Bounds x="190" y="420" width="1170" height="180" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Start Event -->
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="252" y="142" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="232" y="185" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Receive Order -->
      <bpmndi:BPMNShape id="Task_ReceiveOrder_di" bpmnElement="Task_ReceiveOrder">
        <dc:Bounds x="350" y="120" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Check Credit -->
      <bpmndi:BPMNShape id="Task_CheckCredit_di" bpmnElement="Task_CheckCredit">
        <dc:Bounds x="510" y="120" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Gateway -->
      <bpmndi:BPMNShape id="Gateway_Credit_di" bpmnElement="Gateway_Credit" isMarkerVisible="true">
        <dc:Bounds x="675" y="135" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="672" y="111" width="56" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Reject Order -->
      <bpmndi:BPMNShape id="Task_RejectOrder_di" bpmnElement="Task_RejectOrder">
        <dc:Bounds x="810" y="80" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- End Rejected -->
      <bpmndi:BPMNShape id="EndEvent_Rejected_di" bpmnElement="EndEvent_Rejected">
        <dc:Bounds x="982" y="102" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="962" y="145" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Pick & Pack -->
      <bpmndi:BPMNShape id="Task_PickPack_di" bpmnElement="Task_PickPack">
        <dc:Bounds x="650" y="300" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Ship -->
      <bpmndi:BPMNShape id="Task_Ship_di" bpmnElement="Task_Ship">
        <dc:Bounds x="810" y="300" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Invoice -->
      <bpmndi:BPMNShape id="Task_Invoice_di" bpmnElement="Task_Invoice">
        <dc:Bounds x="810" y="470" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Receive Payment -->
      <bpmndi:BPMNShape id="Task_ReceivePayment_di" bpmnElement="Task_ReceivePayment">
        <dc:Bounds x="970" y="470" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- End Complete -->
      <bpmndi:BPMNShape id="EndEvent_Complete_di" bpmnElement="EndEvent_Complete">
        <dc:Bounds x="1142" y="492" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1122" y="535" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Edges -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="288" y="160" />
        <di:waypoint x="350" y="160" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="450" y="160" />
        <di:waypoint x="510" y="160" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="610" y="160" />
        <di:waypoint x="675" y="160" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_CreditOK_di" bpmnElement="Flow_CreditOK">
        <di:waypoint x="700" y="185" />
        <di:waypoint x="700" y="300" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="708" y="233" width="16" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_CreditFailed_di" bpmnElement="Flow_CreditFailed">
        <di:waypoint x="725" y="160" />
        <di:waypoint x="768" y="160" />
        <di:waypoint x="768" y="120" />
        <di:waypoint x="810" y="120" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="755" y="103" width="30" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_ToRejectedEnd_di" bpmnElement="Flow_ToRejectedEnd">
        <di:waypoint x="910" y="120" />
        <di:waypoint x="982" y="120" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="750" y="340" />
        <di:waypoint x="810" y="340" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_5_di" bpmnElement="Flow_5">
        <di:waypoint x="860" y="380" />
        <di:waypoint x="860" y="470" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_6_di" bpmnElement="Flow_6">
        <di:waypoint x="910" y="510" />
        <di:waypoint x="970" y="510" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_7_di" bpmnElement="Flow_7">
        <di:waypoint x="1070" y="510" />
        <di:waypoint x="1142" y="510" />
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
