<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_ProcureToPay"
                  targetNamespace="http://turbo-ea.io/bpmn"
                  exporter="Turbo EA"
                  exporterVersion="1.0">

  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_1" name="Procure to Pay" processRef="Process_1" />
  </bpmn:collaboration>

  <bpmn:process id="Process_1" isExecutable="false">
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_Requester" name="Requester">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CreateRequisition</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Procurement" name="Procurement">
        <bpmn:flowNodeRef>Task_ApproveRequisition</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CreatePO</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Warehouse" name="Warehouse">
        <bpmn:flowNodeRef>Task_ReceiveGoods</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Finance" name="Finance">
        <bpmn:flowNodeRef>Task_InvoiceVerification</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Payment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_1</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <bpmn:startEvent id="StartEvent_1" name="Need Identified">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:userTask id="Task_CreateRequisition" name="Create Requisition">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_ApproveRequisition" name="Approve Requisition">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_CreatePO" name="Create PO">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:userTask id="Task_ReceiveGoods" name="Receive Goods">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_InvoiceVerification" name="Invoice Verification">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_Payment" name="Payment">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:endEvent id="EndEvent_1" name="Payment Complete">
      <bpmn:incoming>Flow_7</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_CreateRequisition" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_CreateRequisition" targetRef="Task_ApproveRequisition" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_ApproveRequisition" targetRef="Task_CreatePO" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_CreatePO" targetRef="Task_ReceiveGoods" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_ReceiveGoods" targetRef="Task_InvoiceVerification" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_InvoiceVerification" targetRef="Task_Payment" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_Payment" targetRef="EndEvent_1" />
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_1">

      <bpmndi:BPMNShape id="Participant_1_di" bpmnElement="Participant_1" isHorizontal="true">
        <dc:Bounds x="160" y="60" width="1050" height="680" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Requester_di" bpmnElement="Lane_Requester" isHorizontal="true">
        <dc:Bounds x="190" y="60" width="1020" height="160" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Procurement_di" bpmnElement="Lane_Procurement" isHorizontal="true">
        <dc:Bounds x="190" y="220" width="1020" height="170" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Warehouse_di" bpmnElement="Lane_Warehouse" isHorizontal="true">
        <dc:Bounds x="190" y="390" width="1020" height="160" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Finance_di" bpmnElement="Lane_Finance" isHorizontal="true">
        <dc:Bounds x="190" y="550" width="1020" height="190" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Start Event -->
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="262" y="122" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="241" y="165" width="78" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Create Requisition -->
      <bpmndi:BPMNShape id="Task_CreateRequisition_di" bpmnElement="Task_CreateRequisition">
        <dc:Bounds x="370" y="100" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Approve Requisition -->
      <bpmndi:BPMNShape id="Task_ApproveRequisition_di" bpmnElement="Task_ApproveRequisition">
        <dc:Bounds x="370" y="265" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Create PO -->
      <bpmndi:BPMNShape id="Task_CreatePO_di" bpmnElement="Task_CreatePO">
        <dc:Bounds x="550" y="265" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Receive Goods -->
      <bpmndi:BPMNShape id="Task_ReceiveGoods_di" bpmnElement="Task_ReceiveGoods">
        <dc:Bounds x="550" y="430" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Invoice Verification -->
      <bpmndi:BPMNShape id="Task_InvoiceVerification_di" bpmnElement="Task_InvoiceVerification">
        <dc:Bounds x="550" y="605" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Payment -->
      <bpmndi:BPMNShape id="Task_Payment_di" bpmnElement="Task_Payment">
        <dc:Bounds x="730" y="605" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- End Event -->
      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <dc:Bounds x="912" y="627" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="886" y="670" width="88" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Edges -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="298" y="140" />
        <di:waypoint x="370" y="140" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="420" y="180" />
        <di:waypoint x="420" y="265" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_3_di" bpmnElement="Flow_3">
        <di:waypoint x="470" y="305" />
        <di:waypoint x="550" y="305" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="600" y="345" />
        <di:waypoint x="600" y="430" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_5_di" bpmnElement="Flow_5">
        <di:waypoint x="600" y="510" />
        <di:waypoint x="600" y="605" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_6_di" bpmnElement="Flow_6">
        <di:waypoint x="650" y="645" />
        <di:waypoint x="730" y="645" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_7_di" bpmnElement="Flow_7">
        <di:waypoint x="830" y="645" />
        <di:waypoint x="912" y="645" />
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
